{
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "module": "ESNext",
    "allowSyntheticDefaultImports": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "strict": true,
    "noPropertyAccessFromIndexSignature": true,
    "downlevelIteration": true,
    "lib": [
      "ESNext"
    ],
    "noImplicitOverride": true
  }
}
