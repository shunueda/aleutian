{
  "name": "kleisli.js",
  "version": "1.0.0",
  "description": "Functional library for JavaScript/Typescript",
  "type": "module",
  "main": "src/index.js",
  "repository": {
    "url": ""
  },
  "scripts": {
    "format": "prettier --ignore-path .gitignore \"**/*.+(ts|js|tsx)\" --write",
    "prebuild": "rm -rf dist",
    "build": "npx swc src -q --out-dir dist/src & npx swc test -q --out-dir dist/test",
    "postbuild": "node scripts/fixImports.js",
    "test": "yarn build && node dist/test"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.23",
    "@types/node": "^18.11.17",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  },
  "author": "Shun Ueda",
  "license": "MIT"
}
